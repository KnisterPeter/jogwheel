{
  "name": "jogwheel",
  "description": "take control over your css keyframe animations",
  "main": "library/index.js",
  "jsnext:main": "source/library/index.js",
  "scripts": {
    "start": "$npm_package_config_jogwheel_task_runner",
    "build": "$npm_package_config_jogwheel_task_runner build",
    "test": "$npm_package_config_jogwheel_when_ci --leader --trusted --no-pull-request && npm run test:remote || npm run test:local",
    "test:local": "parallelshell \"npm run test:local:unit\" \"npm run test:local:browser | tap-spec\"",
    "test:local:unit": "$npm_package_config_jogwheel_task_runner test",
    "test:local:integration": "$npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_integration_test_entry --phantom",
    "test:local:browser": "$npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_unit_test_entry --phantom",
    "test:remote": "parallelshell \"npm run test:remote:unit | tap-spec\" \"npm run test:remote:browser | tap-spec\" && npm run test:remote:integration | tap-spec",
    "test:remote:unit": "$npm_package_config_jogwheel_task_runner test",
    "test:remote:integration": "$npm_package_config_jogwheel_when_ci --leader --trusted && $npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_integration_test_entry || echo ''",
    "test:remote:browser": "$npm_package_config_jogwheel_when_ci --leader --trusted && $npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_unit_test_entry || echo ''",
    "coveralls": "istanbul cover distribution/test/unit/index.js && cat coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "commitmsg": "$npm_package_config_jogwheel_when_ci && $npm_package_config_jogwheel_validate_commit || $npm_package_config_jogwheel_validate_commit \".git/COMMIT_EDITMSG\"",
    "env": "env",
    "binaries": "ls node_modules/.bin/",
    "semantic-release": "(semantic-release pre && npm publish && semantic-release post) && npm run pages || npm run pages:update",
    "pages": "$npm_package_config_jogwheel_when_ci --leader --trusted && (npm run pages:update && npm run pages:archive) || echo ''",
    "pages:update": "node $npm_package_config_jogwheel_pages_update",
    "pages:archive": "node $npm_package_config_jogwheel_pages_archive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marionebl/jogwheel.git"
  },
  "keywords": [
    "css",
    "cssom",
    "keyframe",
    "keyframes",
    "animations",
    "webanimations",
    "static",
    "interpolation"
  ],
  "author": {
    "name": "Mario Nebl",
    "email": "hello@herebecode.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marionebl/jogwheel/issues"
  },
  "homepage": "https://github.com/marionebl/jogwheel#readme",
  "config": {
    "jogwheel": {
      "test-port": 1337,
      "task-runner": "gulp",
      "test-launcher": "zuul --no-coverage",
      "validate-commit": "node distribution/scripts/validate-commit.js",
      "when-ci": "node distribution/scripts/when-ci",
      "unit-test-entry": "distribution/test/unit/index.js",
      "integration-test-entry": "distribution/test/integration/index.js",
      "pages-update": "distribution/scripts/pages-update.js",
      "pages-archive": "distribution/scripts/pages-archive.js"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "documentation": {
      "slug": "marionebl/jogwheel",
      "bugs": "marionebl/jogwheel/issues",
      "icon": "",
      "color": "5ec792",
      "logo": "source/documentation/static/jogwheel.svg",
      "statichost": "https://cdn.rawgit.com"
    },
    "pages": {
      "slug": "marionebl/jogwheel",
      "branch": "gh-pages",
      "image": "static/jogwheel.svg",
      "svgicon": "static/jogwheel.svg",
      "pngicon": "static/jogwheel.png",
      "script": "static/index.js",
      "stylesheet": "static/index.css",
      "archives": "archives",
      "statichost": "https://cdn.rawgit.com"
    }
  },
  "devDependencies": {
    "async": "1.5.0",
    "babel-core": "5.8.33",
    "babel-eslint": "4.1.5",
    "babel-polyfill": "6.0.16",
    "babel-runtime": "5.8.29",
    "babelify": "6.4.0",
    "browserify": "12.0.1",
    "chalk": "1.1.1",
    "commitizen": "2.4.4",
    "coveralls": "2.11.4",
    "cssnano": "3.3.2",
    "cssnext": "1.8.4",
    "cz-conventional-changelog": "1.1.4",
    "del": "2.0.2",
    "denodeify": "1.2.1",
    "dom-stub": "1.0.1",
    "eslint": "1.8.0",
    "eslint-config-xo": "0.7.1",
    "eslint-plugin-babel": "2.1.1",
    "fn-name": "2.0.1",
    "globby": "3.0.1",
    "gulp": "3.9.0",
    "gulp-babel": "5.3.0",
    "gulp-cached": "1.1.0",
    "gulp-data": "1.2.0",
    "gulp-documentation": "2.0.0",
    "gulp-eslint": "1.0.0",
    "gulp-ext-replace": "0.2.0",
    "gulp-istanbul": "0.10.2",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.0.1",
    "gulp-remember": "0.3.0",
    "gulp-rename": "1.2.2",
    "gulp-sequence": "0.4.1",
    "gulp-shell": "0.5.1",
    "gulp-tape": "git+https://github.com/marionebl/gulp-tape.git#93abadf0f130afed6c63910ba0acb790ca01aa37",
    "gulp-template": "3.0.0",
    "gulp-util": "3.0.7",
    "husky": "0.10.1",
    "istanbul": "0.4.0",
    "js-git": "0.7.7",
    "lodash.merge": "3.3.2",
    "mdast": "2.1.0",
    "mdast-highlight.js": "1.2.0",
    "mdast-html": "1.2.1",
    "mdast-inline-links": "1.0.0",
    "mdast-slug": "2.0.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "node-notifier": "4.3.1",
    "normalize.css": "3.0.3",
    "parallelshell": "2.0.0",
    "phantomjs": "1.9.18",
    "postcss": "5.0.11",
    "postcss-cssnext": "2.2.0",
    "postcss-import": "7.1.3",
    "rc": "1.1.5",
    "semantic-release": "4.3.5",
    "shelljs": "0.5.3",
    "tap-spec": "4.1.0",
    "tape": "4.2.2",
    "tape-catch": "1.0.4",
    "unist-util-visit": "1.0.0",
    "web-animations-js": "2.1.3",
    "whatwg-fetch": "0.10.1",
    "zuul": "3.7.2"
  },
  "dependencies": {
    "documentation": "3.0.4"
  }
}
