{
  "name": "jogwheel",
  "version": "1.0.0",
  "description": "take control over your css keyframe animations",
  "main": "library/index.js",
  "jsnext:main": "source/library/index.js",
  "scripts": {
    "start": "$npm_package_config_jogwheel_task_runner",
    "build": "$npm_package_config_jogwheel_task_runner build",
    "test": "parallelshell \"npm run test:local:unit\" \"npm run test:local:browser\" \"npm run test:local:integration\"",
    "test:local:unit": "$npm_package_config_jogwheel_task_runner test",
    "test:local:integration": "$npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_integration_test_entry --phantom",
    "test:local:browser": "$npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_unit_test_entry --phantom",
    "pretest:remote": "npm run build && npm run commitmsg",
    "test:remote": "parallelshell \"npm run test:remote:unit\" \"npm run test:remote:browser\" && npm run test:remote:integration",
    "test:remote:unit": "$npm_package_config_jogwheel_task_runner test",
    "test:remote:integration": "$npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_integration_test_entry",
    "test:remote:browser": "node $npm_package_config_jogwheel_when_ci_leader && $npm_package_config_jogwheel_test_launcher $npm_package_config_jogwheel_unit_test_entry || echo ''",
    "commit": "git-cz",
    "commitmsg": "node $npm_package_config_jogwheel_when_ci_leader && node $npm_package_config_jogwheel_validate_commit || node $npm_package_config_jogwheel_validate_commit \".git/COMMIT_EDITMSG\"",
    "env": "env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marionebl/jogwheel.git"
  },
  "keywords": [
    "css",
    "cssom",
    "keyframe",
    "keyframes",
    "animations",
    "webanimations",
    "static",
    "interpolation"
  ],
  "author": {
    "name": "Mario Nebl",
    "email": "hello@herebecode.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marionebl/jogwheel/issues"
  },
  "homepage": "https://github.com/marionebl/jogwheel#readme",
  "config": {
    "jogwheel": {
      "task-runner": "gulp",
      "test-launcher": "zuul",
      "validate-commit": "distribution/scripts/validate-commit.js",
      "when-ci-leader": "distribution/scripts/when-ci-leader",
      "unit-test-entry": "distribution/test/unit/index.js",
      "integration-test-entry": "distribution/test/integration/index.js"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "documentation": {
      "slug": "marionebl/jogwheel",
      "icon": "",
      "logo": "source/jogwheel.svg"
    }
  },
  "devDependencies": {
    "babel-core": "6.1.2",
    "babel-eslint": "4.1.4",
    "babel-plugin-external-helpers-2": "6.0.15",
    "babel-plugin-transform-async-to-generator": "6.0.14",
    "babel-plugin-transform-class-properties": "6.0.14",
    "babel-plugin-transform-es2015-arrow-functions": "6.0.14",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.0.14",
    "babel-plugin-transform-es2015-block-scoping": "6.0.18",
    "babel-plugin-transform-es2015-classes": "6.1.2",
    "babel-plugin-transform-es2015-computed-properties": "6.0.14",
    "babel-plugin-transform-es2015-constants": "6.0.15",
    "babel-plugin-transform-es2015-destructuring": "6.0.18",
    "babel-plugin-transform-es2015-for-of": "6.0.14",
    "babel-plugin-transform-es2015-function-name": "6.0.14",
    "babel-plugin-transform-es2015-literals": "6.0.18",
    "babel-plugin-transform-es2015-modules-commonjs": "6.1.3",
    "babel-plugin-transform-es2015-object-super": "6.0.14",
    "babel-plugin-transform-es2015-parameters": "6.0.18",
    "babel-plugin-transform-es2015-shorthand-properties": "6.0.14",
    "babel-plugin-transform-es2015-spread": "6.0.14",
    "babel-plugin-transform-es2015-sticky-regex": "6.0.18",
    "babel-plugin-transform-es2015-template-literals": "6.0.14",
    "babel-plugin-transform-es2015-typeof-symbol": "6.1.2",
    "babel-plugin-transform-es2015-unicode-regex": "6.0.18",
    "babel-plugin-transform-strict-mode": "6.1.2",
    "babel-polyfill": "6.0.16",
    "babel-preset-stage-0": "6.1.2",
    "babel-runtime": "6.0.14",
    "chalk": "1.1.1",
    "commitizen": "2.4.1",
    "cz-conventional-changelog": "1.1.4",
    "del": "2.0.2",
    "denodeify": "1.2.1",
    "devlab": "1.5.1",
    "dom-stub": "1.0.1",
    "eslint": "1.8.0",
    "eslint-config-xo": "0.7.1",
    "eslint-plugin-babel": "2.1.1",
    "fn-name": "2.0.1",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.0",
    "gulp-cached": "1.1.0",
    "gulp-data": "1.2.0",
    "gulp-documentation": "2.0.0",
    "gulp-eslint": "1.0.0",
    "gulp-ext-replace": "0.2.0",
    "gulp-istanbul": "0.10.2",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.0.1",
    "gulp-remember": "0.3.0",
    "gulp-rename": "1.2.2",
    "gulp-sequence": "0.4.1",
    "gulp-shell": "0.5.1",
    "gulp-tape": "git+https://github.com/marionebl/gulp-tape.git#93abadf0f130afed6c63910ba0acb790ca01aa37",
    "gulp-template": "3.0.0",
    "gulp-util": "3.0.7",
    "husky": "0.10.1",
    "js-git": "0.7.7",
    "lodash.merge": "3.3.2",
    "node-notifier": "4.3.1",
    "parallelshell": "2.0.0",
    "phantomjs": "1.9.18",
    "rc": "1.1.5",
    "tap-spec": "4.1.0",
    "tape": "4.2.2",
    "tape-catch": "1.0.4",
    "zuul": "3.7.2"
  }
}
